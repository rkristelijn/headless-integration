{
    "version": "0.0.1",
    "manifest_version": 2,
    "author": "Priyank Parashar",
    "default_locale": "en",
    "name": "Headless integration",
    "description": "Testing assistant based on Headless Chrome (Puppeteer)",
    "homepage_url": "https://github.com/webextensions/headless-integration",
    "icons": {
        "16": "icons/icon-16.png",
        "24": "icons/icon-24.png",
        "32": "icons/icon-32.png",
        "40": "icons/icon-40.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png",
        "256": "icons/icon-256.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "optional_permissions": [
        "webNavigation",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "24": "icons/icon-24.png",
            "32": "icons/icon-32.png",
            "40": "icons/icon-40.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png",
            "256": "icons/icon-256.png"
        }
    },
    "background": {
        "persistent": false,
        "page": "background-headless-integration.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+H"
            }
        }
    },
    "offline_enabled": true
}
