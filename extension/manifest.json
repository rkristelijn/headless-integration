{
    "version": "0.0.1",
    "manifest_version": 2,
    "author": "Priyank Parashar",
    "name": "Headless integration",
    "description": "Testing assistant based on Headless Chrome (Puppeteer)",
    "homepage_url": "https://github.com/webextensions/headless-integration",
    "icons": {
        "16": "icons/icon-11x11.ico",
        "24": "icons/icon-11x11.ico",
        "32": "icons/icon-11x11.ico",
        "40": "icons/icon-11x11.ico",
        "48": "icons/icon-11x11.ico",
        "128": "icons/icon-11x11.ico",
        "256": "icons/icon-11x11.ico"
    },
    "permissions": [
        "activeTab",
        "storage",
        "webNavigation",
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icons/icon-11x11.ico",
            "24": "icons/icon-11x11.ico",
            "32": "icons/icon-11x11.ico",
            "40": "icons/icon-11x11.ico",
            "48": "icons/icon-11x11.ico",
            "128": "icons/icon-11x11.ico",
            "256": "icons/icon-11x11.ico"
        },
        "default_title": "Headless integration",
        "default_popup": "browser_action.html"
    },
    "background": {
        "persistent": false,
        "page": "background-headless-integration.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+H"
            }
        }
    },
    "offline_enabled": true
}
